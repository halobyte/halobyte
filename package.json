{
  "name": "halobyte",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build --turbopack",
    "dev": "pnpm with-env next dev --turbopack",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "format:write": "prettier --ignore-path .gitignore --write .",
